
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Domain\Nuptic\Nuptic" table="nuptic">
        <id name="nupticId" type="string" column="id" length="36"/>
        <field name="simulatorId" column="simulator_id" length="255" />
        <field name="num" column="num" type="integer" />
        <field name="direction" column="direction" length="5"/>
        <field name="route" column="route" type="integer"/>
        <field name="createdAt" column="created_at" type="date_immutable"/>
    </entity>
</doctrine-mapping>